{
	"name": "SbiApp",
	"version": "1.0.3",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"start": "react-native start",
		"emulator": "~/Library/Android/sdk/emulator/emulator -avd Medium_Phone_API_36.0",
		"test": "jest",
		"v:patch": "npm version patch --no-git-tag-version && node src/scripts/update-version.js",
		"v:minor": "npm version minor --no-git-tag-version && node src/scripts/update-version.js",
		"v:major": "npm version major --no-git-tag-version && node src/scripts/update-version.js",
		"build:apk": "npm run build:apk:prepare && cd android && ./gradlew assembleRelease && cd .. && node src/scripts/copy-apk.js",
		"build:apk:prepare": "node src/scripts/update-version.js",
		"build:expo:ios": "eas build --platform ios --profile production"
	},
